@page
@using Net.Leksi.Pocota.Tool.Pages
@using Net.Leksi.Pocota.Tool
@model ServerModelModel
@{
}
@Html.Partial("Header", Model)
public class @Model.ClassName@(Model.Inheritances.Count > 0 ? $": {string.Join(", ", Model.Inheritances)}" : string.Empty)
{
@foreach(PropertyModel pm in Model.Properties)
{
    <text>    @foreach (string attr in pm.Attributes)
    {
        <text>    @attr
</text>
    }
    public @Html.Raw(pm.TypeName)@(pm.IsNullable ? '?' : string.Empty) @pm.Name { get; set; }
</text>
}
}
